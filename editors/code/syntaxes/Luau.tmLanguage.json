            }
          },
          "end": "(\\))",
          "endCaptures": {
            "1": {
              "name": "punctuation.definition.parameters.end.luau"
            }
          },
          "name": "meta.parameter.luau",
          "patterns": [
            {
              "include": "#comment"
            },
            {
              "match": "\\.\\.\\.",
